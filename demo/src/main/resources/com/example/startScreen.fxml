<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="200.0" minWidth="290.0" spacing="20.0" style="-fx-background-color: #1004ba;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.StartScreenController">
   <children>
      <Label text="Dumb&amp;Dumber" textFill="#dddddd">
         <font>
            <Font name="TeXGyreAdventor-Regular" size="20.0" />
         </font></Label>
      <Button fx:id="startGameButton" mnemonicParsing="false" onAction="#startGame" prefHeight="26.0" prefWidth="125.0" style="-fx-background-color: #8682ba;" text="Start Game" textFill="#dddddd" />
      <Button fx:id="playersButton" onAction="#openPlayers" prefHeight="26.0" prefWidth="125.0" style="-fx-background-color: #8682ba;" text="Players" textAlignment="JUSTIFY" textFill="#dddddd" />
      <Button fx:id="loadQuestionsButton" mnemonicParsing="false" onAction="#loadQuestions" prefHeight="26.0" prefWidth="125.0" style="-fx-background-color: #8682ba;" text="Load Questions" textAlignment="JUSTIFY" textFill="#dddddd" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
